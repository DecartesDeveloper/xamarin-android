<?xml version="1.0" encoding="utf-8"?>
<Project>
  <!--NOTE: manually importing Sdk.props and Sdk.targets so we can replace the Pack target-->
  <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />

  <PropertyGroup>
    <TargetFrameworks>net472;netcoreapp3.1</TargetFrameworks>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <OutputType>Exe</OutputType>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <RootNamespace>setupwindows</RootNamespace>
    <AssemblyName>setup-windows</AssemblyName>
    <OutputPath>$(XamarinAndroidSourcePath)bin\$(Configuration)-$(TargetFramework)\</OutputPath>
    <DefaultItemExcludes>$(DefaultItemExcludes);**\.DS_Store</DefaultItemExcludes>
  </PropertyGroup>
 
  <Import Project="..\..\Configuration.props" />

  <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
  <Target Name="Pack" />

</Project>